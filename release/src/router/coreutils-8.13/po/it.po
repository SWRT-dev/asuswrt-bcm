msgstr "larghezza del campo non valida: %s"

#: src/printf.c:583
#, c-format
msgid "invalid precision: %s"
msgstr "precisione non valida: %s"

#: src/printf.c:610
#, c-format
msgid "%.*s: invalid conversion specification"
msgstr "%.*s: specificatore di conversione non valido"

#: src/printf.c:693
