#, c-format
msgid "Error spawning command line '%s': "
msgstr "Kļūda, izveidojot komandrindu “%s”: "

#: ../gio/gdbusaddress.c:1296
#, c-format
msgid "(Type any character to close this window)\n"
msgstr "(Ievadiet jebkuru rakstzīmi, lai aizvērtu šo logu)\n"

