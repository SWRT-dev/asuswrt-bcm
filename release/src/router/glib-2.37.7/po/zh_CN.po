#: ../gio/gcharsetconverter.c:349 ../glib/gconvert.c:772
#: ../glib/gconvert.c:1081 ../glib/giochannel.c:1593 ../glib/giochannel.c:2484
#, c-format
msgid "Error during conversion: %s"
msgstr "转换过程中出错：%s"

#: ../gio/gcharsetconverter.c:446 ../gio/gsocket.c:991
