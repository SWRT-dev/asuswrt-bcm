   0x87    0x80    True                  True
   0x88    0x9D    True                  True
   0x89    0xD3    True                  True
   0x8B    0x8A    True                  True
   0x8C    0xD7    True                  True
   0x92    0x91    True                  True
   0x93    0xE2    True                  True
   0x94    0x99    True                  True
   0x96    0x95    True                  True
   0x98    0x97    True                  True
   0x9C    0x9B    True                  True
   0x9F    0xAC    True                  True
   0xA0    0xB5    True                  True
   0xA1    0xD6    True                  True
   0xA2    0xE0    True                  True
   0xA3    0xE9    True                  True
   0xA5    0xA4    True                  True
   0xA7    0xA6    True                  True
   0xA9    0xA8    True                  True
