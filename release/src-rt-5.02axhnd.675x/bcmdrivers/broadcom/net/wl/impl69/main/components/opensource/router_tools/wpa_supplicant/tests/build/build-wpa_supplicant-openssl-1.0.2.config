CONFIG_TLS=openssl
OPENSSLDIR=/usr/local/openssl/1.0.2q
CFLAGS += -I$(OPENSSLDIR)/include
LIBS += -L$(OPENSSLDIR)/lib
LDFLAGS += -Wl,-rpath=$(OPENSSLDIR)/lib
CONFIG_TLS_ADD_DL=y

CONFIG_WPS=y
CONFIG_EAP_TLS=y
CONFIG_EAP_MSCHAPV2=y

CONFIG_EAP_PSK=y
CONFIG_EAP_GPSK=y
CONFIG_EAP_AKA=y
CONFIG_EAP_SIM=y
CONFIG_EAP_SAKE=y
CONFIG_EAP_PAX=y
CONFIG_EAP_FAST=y
CONFIG_EAP_IKEV2=y

CONFIG_SAE=y
CONFIG_FILS=y
CONFIG_FILS_SK_PFS=y
CONFIG_OWE=y
CONFIG_DPP=y
CONFIG_SUITEB=y
CONFIG_SUITEB192=y

CFLAGS += -Werror
