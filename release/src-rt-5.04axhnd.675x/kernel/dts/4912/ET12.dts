};

&nand {
	write-protect = <1>;
};

&pcie0 {
	status = "okay";
};
&pcie1 {
	status = "okay";
};
&pcie2 {
	status = "okay";
};
&pcie3 {
	status = "okay";
};

&usb_ctrl {
	pinctrl-names = "default";
	pinctrl-0 = <&usb0_pwr_pins &usb1_pwr_pins>;
	status = "okay";
	pwron-bias-pull-up;
	xhci-enable;
};

&usb0_xhci {
	status = "okay";
};

&mdio {
	phy1:1 {
		status = "okay";
	};

	phy2:2 {
		status = "okay";
	};

	phy_cascade0:9 {
		status = "okay";
	};

	phy_serdes0:6 {
		phy-handle = <&phy_cascade0>;
		status = "okay";
	};

	phy_cascade1:10 {
		reg = <19>;
		status = "okay";
	};

	phy_serdes1:7 {
		phy-handle = <&phy_cascade1>;
		status = "okay";
	};
};

&switch0 {
	ports {
		port0@0 {
			status = "okay";
			label = "eth2";
		};

		port1@1 {
			status = "okay";
			label = "eth1";
		};

		port5@5 {
			status = "okay";
			label = "eth0";
		};

		port6@6 {
			status = "okay";
			label = "eth3";
		};
	};
};

&led_ctrl {
    status="okay";
	bt_reset:gpio_3_pin_3 {
		label = "led_gpio_3";
		compatible = "brcm,gpio-led";
		software_led;
		pin = <3>;
		active_high;
		init_low;
	};

