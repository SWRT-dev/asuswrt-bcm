		active_high;
		link = <(LED_SPEED_100|LED_SPEED_1G)>;
		status = "okay";
	};
	led18: sw_led_18-pin_18 {
		active_high;
		status = "okay";
	};
	led19: sw_led-bit_19 {
		active_high;
		status = "okay";
	};
	led20: port_0-led_3-bit_20 {
		active_high;
		activity = <LED_SPEED_GBE>;
		link = <(LED_SPEED_GBE)>;
		status = "okay";
	};
	led21: port_1-led_3-bit_21 {
		active_high;
		activity = <LED_SPEED_GBE>;
		link = <(LED_SPEED_GBE)>;
		status = "okay";
	};
	led22: port_2-led_3-bit_22 {
		active_high;
		activity = <LED_SPEED_GBE>;
		link = <(LED_SPEED_GBE)>;		
		status = "okay";
	};
