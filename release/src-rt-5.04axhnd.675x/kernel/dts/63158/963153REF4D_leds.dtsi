		active_high;
		activity = <LED_SPEED_GBE>;
		link = <(LED_SPEED_GBE)>;
		status = "okay";
	};
	led25: cb_port_0-led_3-bit_25 {
		active_high;
		activity = <(LED_SPEED_GBE|LED_SPEED_2500)>;
		link = <(LED_SPEED_GBE|LED_SPEED_2500)>;
		status = "okay";
	};
	/* software led for GPON ranging activity */
	/* led26: sw_led-bit_26 {
		active_high;
		status = "okay";
	}; */

	led26: rnr_port_4-led_3-bit_26 {
		active_high;
		activity = <(LED_SPEED_GBE|LED_SPEED_2500|LED_SPEED_10G)>;
		link = <(LED_SPEED_GBE|LED_SPEED_2500|LED_SPEED_10G)>;
		status = "okay";
	};

	led27: rnr_port_5-led_3-bit_27 {
		active_high;
		activity = <LED_SPEED_GBE>;
		link = <(LED_SPEED_GBE)>;
		status = "okay";
	};
	led28: sw_led_28-pin_28 {
		active_high;
		status = "okay";
	};
	led29: sw_led_29-pin_29 {
		active_high;
		status = "okay";
	};
	led30: dsl_act_0-pin_30 {
		active_high;
		status = "okay";
	};
	led31: dsl_act_1-pin_31 {
		active_high;
		status = "okay";
	};
};
