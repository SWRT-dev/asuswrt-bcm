&i2c0 {
	pinctrl-names="default";
	pinctrl-0 = <&i2c_sda_pin_16 &i2c_scl_pin_17>;
	status = "okay";
	
	#address-cells = <1>;
	#size-cells = <0>;
	codec_playback:codec_palyback@18{
		compatible = "ti,tlv320dac3203";
		reg = <0x18>;
	};
	codec_capture:codec_capture@19{
		compatible = "ti,tlv320adc3101";
		reg = <0x19>;
	};
};


&mdio_sf2 {
	phy_ge:8 {
		status = "okay";
	};
	phy_rgmii:rgmii {
		reg = <25>;
	phy-reset = <&gpioc 10 GPIO_ACTIVE_LOW>;
		status = "okay";
	};
};

&switch0 {
	pinctrl-names = "default", "rgmii";
	pinctrl-0 = <&a_rgmii_mdc_pin_68 &a_rgmii_mdio_pin_69 &rgmii_pins>;
	pinctrl-1 = <&rgmii_pins>;

	ports {
		port0@0 {
			phy-handle = <&phy_ge>;
			status = "okay";
		};
    };
};

&legacy_leds {
	wl-sess-led = <&led4>;
	wl0-led = <&led3>;
	wl1-led = <&led3>;
};
&led_ctrl {
    status="okay";

	led0: port_0-led-0-pin_0 {
		hardware_led;
		active_high;
		link = <(LED_SPEED_100|LED_SPEED_1G)>;
		status = "okay";
	};
	led1: port_0-led-3-pin_3 {
		hardware_led;
		active_low;
		activity = <LED_SPEED_GBE>;
		status = "okay";
	};
	led2: sw_led_01-pin_1 {
		active_low;
		status = "okay";
	};
	led3:sw_led_14 {
		pinctrl-0 = <&a_per_led_14_pin_14>;
		active_low;
		status="okay";
		brightness = <255>;
	};
	led4:sw_led_15 {
		pinctrl-0 = <&a_per_led_15_pin_15>;
		active_low;
		status="okay";
		brightness = <255>;
	};
	led5:sw_led_05 {
		pinctrl-0 = <&a_per_led_05_pin_5>;
		active_low;
		status="okay";
		brightness = <255>;
	};
	led6:sw_led_07 {
		pinctrl-0 = <&a_per_led_07_pin_7>;
		active_low;
		status="okay";
		brightness = <255>;
	};
	led7:sw_led_11 {
		pinctrl-0 = <&a_per_led_11_pin_11>;
		active_low;
		status="okay";
		brightness = <255>;
	};
	led8:sw_led_12 {
		pinctrl-0 = <&a_per_led_12_pin_12>;
		active_high;
		status="okay";
		brightness = <0>;
	};
};



