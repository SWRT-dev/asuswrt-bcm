WLPROBRESP_SW=1
WLPROBRESP_MAC_FILTER=1
WLAUTHRESP_MAC_FILTER=1

# BCMEXTNVM = 1, is used for Windows internal & external driver debug & BU.
# It enables NVRAM file read for the external & internal Windows drivers,
# which otherwise is prohibited. Hence by default set to 0.
BCMEXTNVM=0

BCMECICOEX=1
#ifndef LINUX_CRYPTO
LINUX_CRYPTO=0
#endif

PROPRIETARY_11N_RATES=0

BCM_DMA_CT=1
BCM_DMA_INDIRECT=1

# For BCM47XX platforms
ifneq ($(CONFIG_MACH_BRCM_NS),)
BCMDMASGLISTOSL=1
endif

WL_RATELINKMEM=1
HNDBME=1
HNDDMA=1
BULKRXDMA=1
WL_SCAN_DFS_HOME=1
AVS=1
TXSHAPER=1

#enable tempsense by default
BCMDBG_TEMPSENSE=1

# STS_FIFO_RXEN=1
# disable BME_OFFLOADS_RXSTS for CMWIFI builds.
# WLC_OFFLOADS_RXSTS=1

# Tx Status Transfer using M2M DMA feature
STS_XFER_TXS=1
# PhyRx Status Transfer over Rx FIFO-3 feature
STS_XFER_PHYRXS_FIFO=1
# PhyRx Status Transfer using M2M DMA feature
STS_XFER_PHYRXS_M2M=1

#64 byte TX status support
TX_STATUS_MACTXS_64BYTES=1

